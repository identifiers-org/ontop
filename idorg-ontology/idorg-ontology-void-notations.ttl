@prefix idot: <http://identifiers.org/idot/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix voidext: <http://ldf.fi/void-ext#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

<http://identifiers.org/idot/> 
    owl:imports sh: ;
    owl:imports dcat: ;
    sh:declare [
        sh:prefix "idot" ;
        sh:namespace "http://identifiers.org/idot/"^^xsd:anyURI ;
    ] ;
    sh:declare [
        sh:prefix "idorg" ;
        sh:namespace "http://identifiers.org/"^^xsd:anyURI ;
    ] ;
    sh:declare [
        sh:prefix "dct" ;
        sh:prefix "dcterms" ;
        sh:namespace "http://purl.org/dc/terms/"^^xsd:anyURI ;
    ] ;
    sh:declare [
        sh:prefix "dcat" ;
        sh:namespace "http://www.w3.org/ns/dcat#"^^xsd:anyURI ;
    ] ;
    sh:declare [
        sh:prefix "rdfs" ;
        sh:namespace "http://www.w3.org/2000/01/rdf-schema#"^^xsd:anyURI ;
    ] .



#################################################################
#    Void notations for triple auto completion
#################################################################

